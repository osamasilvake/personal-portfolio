{
	"name": "personal-portfolio",
	"version": "0.3.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build && next-on-netlify",
		"start": "next start",
		"lint": "next lint",
		"pretty:check": "prettier --check ./**/*.{js,jsx,ts,tsx,json}",
		"pretty:apply": "prettier --write ./**/*.{js,jsx,ts,tsx,json}",
		"lint:es": "next lint --quiet",
		"lint:es:fix": "yarn lint:es --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es",
		"release": "release-it",
		"release:major": "yarn release-it -- major --ci",
		"release:minor": "yarn release-it -- minor --ci",
		"release:patch": "yarn release-it -- patch --ci",
		"prepare": "husky"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.18",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.11",
		"moment": "^2.30.1",
		"next": "14.0.4",
		"next-intl": "^3.12.0",
		"next-on-netlify": "^3.0.1",
		"next-themes": "^0.2.1",
		"npm-check-updates": "^17.1.3",
		"react": "^18",
		"react-dom": "^18",
		"sass": "^1.77.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.0.3",
		"@commitlint/config-conventional": "^19.0.3",
		"@next/eslint-plugin-next": "^14.0.4",
		"@release-it/conventional-changelog": "^8.0.1",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^8.8.0",
		"@typescript-eslint/parser": "^8.8.0",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^9.0.11",
		"postcss": "^8",
		"prettier": "^3.1.1",
		"react-icons": "^5.0.1",
		"release-it": "^17.0.1",
		"tailwindcss": "^3.3.0",
		"typescript": "^5"
	}
}
