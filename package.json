{
	"name": "personal-portfolio",
	"version": "0.3.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"test": "vitest",
		"lint": "next lint",
		"pretty:check": "prettier --check ./**/*.{js,jsx,ts,tsx,json}",
		"pretty:apply": "prettier --write ./**/*.{js,jsx,ts,tsx,json}",
		"lint:es": "next lint --quiet",
		"lint:es:fix": "yarn lint:es --fix",
		"scan:all": "yarn pretty:apply && yarn lint:es",
		"release": "release-it",
		"release:major": "yarn release-it -- major --ci",
		"release:minor": "yarn release-it -- minor --ci",
		"release:patch": "yarn release-it -- patch --ci",
		"prepare": "husky"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"dayjs": "^1.11.13",
		"moment": "^2.30.1",
		"next": "14.2.15",
		"next-intl": "^3.22.0",
		"next-on-netlify": "^3.0.1",
		"next-themes": "^0.3.0",
		"npm-check-updates": "^17.1.4",
		"react": "^18",
		"react-dom": "^18",
		"sass": "^1.80.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.5.0",
		"@commitlint/config-conventional": "^19.5.0",
		"@next/eslint-plugin-next": "^14.2.15",
		"@release-it/conventional-changelog": "^9.0.1",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.2",
		"@testing-library/react": "^16.0.1",
		"@types/node": "^22",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^8.11.0",
		"@typescript-eslint/parser": "^8.11.0",
		"@vitejs/plugin-react": "^4.3.3",
		"autoprefixer": "^10.4.20",
		"eslint": "^8",
		"eslint-config-next": "14.2.15",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unused-imports": "^3.0.0",
		"husky": "^9.1.6",
		"jsdom": "^25.0.1",
		"postcss": "^8",
		"prettier": "^3.3.3",
		"react-icons": "^5.3.0",
		"release-it": "^17.10.0",
		"tailwindcss": "^3.4.14",
		"typescript": "^5",
